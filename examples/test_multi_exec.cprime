// Test multiple exec aliases with different execution orders and parameters

// First exec alias - generates variable declarations
exec var_gen<type, name, value> {
    -- Generate variable declaration with initialization
    local var_type = params[0] or "int"
    local var_name = params[1] or "var"
    local var_value = params[2] or "0"
    
    return var_type .. " " .. var_name .. " = " .. var_value .. ";"
}

// Second exec alias - generates function declarations  
exec func_gen<return_type, func_name, param_list> {
    -- Generate function declaration
    local ret_type = params[0] or "void"
    local name = params[1] or "func"
    local params_str = params[2] or ""
    
    return ret_type .. " " .. name .. "(" .. params_str .. ") { return; }"
}

int main() {
    // Call exec aliases in different orders with different parameters
    
    // First call: generate string variable
    var_gen<string, message, "Hello World">();
    
    // Second call: generate function
    func_gen<int, calculate, "int a, int b">();
    
    // Third call: generate integer variable  
    var_gen<int, counter, 42>();
    
    // Fourth call: generate another function
    func_gen<void, process, "string input">();
    
    // Regular code
    print("Multi-exec test completed");
}